.PHONY: help

run:
	# Execute polymorphic.go
	go run ./polymorphic.go
help:
	@echo "Usage: make [target]"
	@echo ""
	@echo "This makefile has one target: run."
	@echo "It executes the polymorphic.go program that demonstrates polymorphism in Go."
	@echo ""
	@echo "Available target:"
	@echo "  run  - Run the polymorphic.go program"
	@echo ""